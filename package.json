{
  "name": "taro-cli",
  "version": "1.0.0",
  "private": true,
  "description": "taro-cli",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "npm runlint && taro build --type weapp",
    "build:swan": "npm runlint && taro build --type swan",
    "build:alipay": "npm runlint && taro build --type alipay",
    "build:tt": "npm runlint && taro build --type tt",
    "build:h5": "npm runlint && taro build --type h5",
    "build:rn": "npm runlint && taro build --type rn",
    "build:qq": "npm runlint && taro build --type qq",
    "build:jd": "npm runlint && taro build --type jd",
    "build:quickapp": "npm runlint && taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "cross-env LINT_MODE=prod eslint \"**/*.{js,jsx,ts,tsx}\" && npm run lint:css",
    "lint:rep": "cross-env LINT_MODE=prod eslint \"**/*.{js,jsx,ts,tsx}\" -o ./.eslintReport.txt",
    "lint:fix": "cross-env LINT_MODE=prod eslint --fix \"**/*.{js,jsx,ts,tsx}\" && npm run lint:css:fix",
    "lint:css": "stylelint \"**/*.{css,less,scss,sass}\" ",
    "lint:css:fix": "stylelint \"**/*.{css,less,scss,sass}\" --fix",
    "lint:css:rep": "stylelint \"**/*.{css,less,scss,sass}\" --fix -o .stylelintReport.txt"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.scss": "stylelint --syntax scss",
    "**/*.{.tsx,.js.ts}": [
      "eslint --fix --ext .tsx",
      "git add"
    ]
  },
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@rematch/core": "^2.0.1",
    "@tarojs/components": "3.2.1",
    "@tarojs/react": "3.2.1",
    "@tarojs/runtime": "3.2.1",
    "@tarojs/taro": "3.2.1",
    "deepmerge": "^4.2.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.2.1",
    "@tarojs/webpack-runner": "3.2.1",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.2.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-shide": "^1.0.0",
    "eslint-plugin-taro": "^3.2.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "stylelint": "^13.12.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-shide": "^1.0.0",
    "typescript": "^4.1.0"
  }
}